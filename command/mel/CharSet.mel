global proc CharSet(){	string $cam[]=`ls -cameras`;	string $ccam;	string $proPath=`workspace -q -fn`;	sysFile -md ($proPath+"/PlayBlast"); 	for($ccam in $cam)	{				if(`attributeExists "in" $ccam`)		{						string $selectcam="lookThroughModelPanel "+$ccam+" modelPanel1";			eval($selectcam);			string $camin="getAttr "+$ccam+".in";			string $camout="getAttr "+$ccam+".out";			int $cammin=eval($camin);			int $cammout=eval($camout);			string $name=$proPath+"/PlayBlast"+"/"+$ccam;			string $select="select "+$ccam;			eval($select);	//			string $CharSet="character -name "+"\""+$ccam+"\""+" -excludeVisibility -excludeScale";			string $CharSet="character  -excludeVisibility -excludeScale";			eval($CharSet);								}	}}CharSet()							