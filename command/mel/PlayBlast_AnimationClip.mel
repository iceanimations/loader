global proc PlayBlast_AnimationClip(){	string $cam[]=`ls -cameras`;	string $ccam;	string $proPath=`workspace -q -fn`;	sysFile -md ($proPath+"/PlayBlast"); 	for($ccam in $cam)	{				if(`attributeExists "in" $ccam`)		{						string $selectcam="lookThroughModelPanel "+$ccam+" modelPanel1";			eval($selectcam);			addCameraInfoHUD();			string $camin="getAttr "+$ccam+".in";			string $camout="getAttr "+$ccam+".out";			int $cammin=eval($camin);			int $cammout=eval($camout);			string $name=$proPath+"/PlayBlast"+"/"+$ccam;			playblast -st $cammin -et $cammout  -format movie -filename $name -forceOverwrite  -clearCache 1 -viewer 1 -showOrnaments 1 -fp 4 -percent 50 -compression none;			string $select="select "+$ccam;			eval($select);				string $CharSet="character -name "+"\""+$ccam+"\""+" -excludeVisibility -excludeScale";			eval($CharSet);			string $clipp="clip -name "+"\""+$ccam+"\""+ " -sc 1 -defaultAbsolute -animCurveRange"+$ccam;			//eval($clipp);			//clip -name "z" -sc 1 -defaultAbsolute -animCurveRange character2;					}	}}//PlayBlast_AnimationClip()