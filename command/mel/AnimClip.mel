global proc AnimClip(){	string $cam[]=`ls -cameras`;	string $ccam;	string $proPath=`workspace -q -fn`;	sysFile -md ($proPath+"/PlayBlast"); 	for($ccam in $cam)	{				if(`attributeExists "in" $ccam`)		{						string $selectcam="lookThroughModelPanel "+$ccam+" modelPanel1";			eval($selectcam);			addCameraInfoHUD();			string $camin="getAttr "+$ccam+".in";			string $camout="getAttr "+$ccam+".out";			int $cammin=eval($camin);			int $cammout=eval($camout);			string $name=$proPath+"/PlayBlast"+"/"+$ccam;			string $clipp="clip -name "+"\""+$ccam+"\""+ " -sc 1 -defaultAbsolute -animCurveRange"+$ccam;			eval($clipp);				}	}}//AnimClip()